<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="aihuhu.framework.data" type="aihuhu.framework.data.Configuration.Exports.FrameworkDataConfigurationSection,aihuhu.framework.data"/>
  </configSections>
  <!--
配置数据库相关配置文件路径，优先使用该配置
如果该配置节点不存在，则从appSettings中读取
-->
  <aihuhu.framework.data databaseFilePath="Configuration/Database.config" commandFilePath="Configuration/DataCommndFileList.config" />

  <appSettings>
    <add key="databaseFilePath" value="Configuration/Database.config"/>
    <add key="commandFilePath" value="Configuration/DataCommndFileList.config"/>
  </appSettings>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>